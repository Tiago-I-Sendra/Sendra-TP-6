1) ingresar un vector de 8 elementos y informe:
valor acumulado total.
valor acumulado de 36>
cantidad de 50>


#include<iostream>
#include<vector>

////////////////////////////////
int AcumulacionTotal (vector<int> A)
{
int B = 0;
for(int i,i<=8,i++)
 {
B = B + A(i);
 }

cout<<"la acumulacion total es:"<<B<<endl;
}

////////////////////////////////
int AcumulacionSeis (vector<int> A)
{
int B = 0;
for(int i,i<=8,i++)
 {

if =(A(i) => 36){B = B + A(i);}

 }

cout<<"la acumulacion sobre 36 es:"<<B<<endl;
}

////////////////////////////////
int AcumulacionCinco (vector<int> A)
{
int B = 0;
for(int i,i<=8,i++)
 {

if =(A(i) => 50){B = B + A(i);}

 }

cout<<"la acumulacion de sobre 50 es:"<<B<<endl;
}

////////////////////////////////
int main(int argc, char *argv[])
{

vector<int> Elementos (8);

for(int i = 0,i<=8,i++)
{
cin>>Elementos (i);
}

AcumulaciónTotal (Elementos);

AcumulaciónSeis (Elementos);

AcumulacionCinco (Elementos);
}
